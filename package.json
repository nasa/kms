{
  "name": "kms",
  "version": "0.0.1",
  "proxy": "http://localhost:8080",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "lint": "eslint . --ext .jsx,.js,.ts,.tsx",
    "lint:fix": "eslint --fix \"serverless/src/**/*.{js,ts,jsx,tsx}\"",
    "preview": "vite preview",
    "test": "vitest",
    "cdk": "cdk",
    "run-synth": "cd cdk && cdk synth --context useLocalstack=true --output ./cdk.out",
    "start-local": "bin/start-local.sh",
    "deploy-application": "cd cdk && npm ci --include=dev && npx cdk deploy KmsStack --progress events --require-approval never",
    "rdf4j:build": "docker build -t rdf4j:latest ./cdk/rdfdb/docker",
    "rdf4j:start": "docker run -d -p 8080:8080 -e RDF4J_USER_NAME=$RDF4J_USER_NAME -e RDF4J_PASSWORD=$RDF4J_PASSWORD -e JAVA_OPTS='-Xms6.5g -Xmx6.5g' -v logs:/usr/local/tomcat/logs rdf4j:latest",
    "rdf4j:stop": "docker ps -q --filter ancestor=rdf4j:latest | xargs -r docker kill && docker ps -aq --filter ancestor=rdf4j:latest | xargs -r docker rm",
    "rdf4j:setup": "node setup/setupRdf4j.js",
    "rdf4j:shell": "docker exec -it $(docker ps -q --filter ancestor=rdf4j:latest) /bin/bash",
    "export-data": "vite-node --config vite.config.js setup/scripts/exportData.js -all",
    "load-export": "vite-node --config vite.config.js setup/scripts/loadExport.js -all",
    "create-rdf-files": "vite-node --config vite.config.js setup/scripts/createRdfFiles.js"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.775.0",
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/client-sfn": "^3.775.0",
    "@xmldom/xmldom": "^0.8.10",
    "compact-object-deep": "^1.0.0",
    "csv": "^6.3.11",
    "date-fns": "^4.1.0",
    "fast-xml-parser": "^4.5.1",
    "html-entities": "^2.5.2",
    "html-escaper": "^3.0.3",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.7.0",
    "remove": "^0.1.5",
    "serverless": "^3.35.2",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.48.5",
    "serverless-finch": "^4.0.3",
    "serverless-offline": "^13.2.0",
    "serverless-s3-local": "^0.8.1",
    "serverless-step-functions": "^3.22.0",
    "stream": "^0.0.3",
    "uuid": "^11.1.0",
    "vite": "^4.5.5",
    "xpath": "^0.0.34"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.24.1",
    "@edsc/eslint-config": "^0.0.5",
    "@smithy/util-stream": "^3.0.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-istanbul": "^1.4.0",
    "aws-cdk-lib": "^2.210.0",
    "aws-sdk-client-mock": "^4.0.0",
    "constructs": "^10.4.2",
    "esbuild": "^0.19.12",
    "esbuild-node-externals": "^1.18.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-newlines": "^1.3.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "jsdom": "^22.1.0",
    "postcss-scss": "^4.0.9",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "start-server-and-test": "^2.0.2",
    "stylelint": "^15.10.3",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-scss": "^5.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vitest": "^1.4.0"
  }
}
